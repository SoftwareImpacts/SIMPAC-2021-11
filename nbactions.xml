            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xmx10g -classpath %classpath org.thema.graphab.MainFrame -proc 3 --project dijon_foret/foret.xml  --linkset distance=cost name=foret_planaire 11,12,13=1000 14,15,21,31,32,41=100 22=10 331,332,341,342,35=1</exec.args>
